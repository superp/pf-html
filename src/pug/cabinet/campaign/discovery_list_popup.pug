extends ../_layout

block content
  .top-block.top-block__discovery
    .back.desk-hide
      img(src="assets/images/cabinet/arrow_left.svg", alt="")
    .tabs.tabs__campaign
      .tabs_list
        ul
          li.tab-inst
            a(href="#tab-1") Instagram
          li.tab-youtube
            a.active(href="#tab-2") Youtube
    
    .filter-button
      span
        img(src="assets/images/cabinet/filter_icon.svg", alt="")
      span Filter

    .popup-overlay
      .filter_popup.filter_popup__list
        form(action="")

          .form-item.form-item__search-results
            span Add to list
            .buttons
              button.apply-button
                span SAVE
            .close
              span.icon.icon__close
          .form-item.form-item__search
            input.search(type="text")
            button.create-list
              | + Create list
          .lists
            .form-item.lists_item
              input#rozetka_checkbox.checkbox(type="checkbox" name="lists_checkbox")
              label(for="rozetka_checkbox") Rozetka
              span 21 Influencers
            .form-item.lists_item
              input#parimatch_checkbox.checkbox(type="checkbox" name="lists_checkbox")
              label(for="parimatch_checkbox") PariMatch
              span 33 Influencers
            .form-item.lists_item
              input#dmarket_checkbox.checkbox(type="checkbox" name="lists_checkbox")
              label(for="dmarket_checkbox") DMarket
              span 1 Influencers
            .form-item.lists_item
              input#eva_checkbox.checkbox(type="checkbox" name="lists_checkbox")
              label(for="eva_checkbox") EVA
              span 21 Influencers
            .form-item.lists_item
              input#mcdonalds_checkbox.checkbox(type="checkbox" name="lists_checkbox")
              label(for="mcdonalds_checkbox") McDonalds
              span 21 Influencers

