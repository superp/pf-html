              label(for="language_checkbox9") English
            .form-item_subitem
              input#language_checkbox10.checkbox(type="checkbox" name="language_checkbox")
              label(for="language_checkbox10") English
            .form-item_subitem
              input#language_checkbox11.checkbox(type="checkbox" name="language_checkbox")
              label(for="language_checkbox11") Ukrainian
            .form-item_subitem
              input#language_checkbox12.checkbox(type="checkbox" name="language_checkbox")
              label(for="language_checkbox12") Turkish
          
          .form-item.form-item__age
            label Age
            input(type="text" placeholder="10")
            span -
            input(type="text" placeholder="13")

          .form-item.form-item__email
            label With Email
            input#email_checkbox1.checkbox(type="checkbox" name="email_checkbox")

