extends ../_layout

block content
  .top-block.top-block__discovery
    .back.desk-hide
      img(src="assets/images/cabinet/arrow_left.svg", alt="")
    .tabs.tabs__campaign
      .tabs_list
        ul
          li.tab-inst
            a(href="#tab-1") Instagram
          li.tab-youtube
            a.active(href="#tab-2") Youtube
    
    .filter-button
      span
        img(src="assets/images/cabinet/filter_icon.svg", alt="")
      span Filter

    .popup-overlay(style="display: none;")
      .filter_popup
        form(action="")

          .form-item.form-item__search-results
            span Finded 100,000 influencers
            .buttons 
              .hide-filter
                | Hide
              button.apply-button
                span.icon.icon__check
                span Apply
          .form-item.form-item__search
            input.search(type="text")
          .form-item.form-item__budget
            .form-item_subitem
              label Total budget
              select
                option(value="") USD ($)
                option(value="") EUR (‚Ç¨)
                option(value="") CAD ($)
              input(type="text" placeholder="0.00")
            .form-item_subitem
              label Price per influencer collaboration
              select
                option(value="") USD ($)
                option(value="") EUR (‚Ç¨)
                option(value="") CAD ($)
              input(type="text" placeholder="0.00")
                    
          .form-item.form-item__subscribers
            label Subscribers
            input(type="text" placeholder="10")
            span -
            input(type="text" placeholder="13")
          
          .form-item.form-item__engagement
            label Engagement (%)
            input(type="text" placeholder="10")
            span -
            input(type="text" placeholder="13")

          .form-item.form-item__audience
            .form-item_subitem
              label Country
              input.input_tag(type="text")
            .form-item_subitem
              label City
              input.input_tag(type="text")

          .form-item.form-item__gender
            label Gender
            
            .form-item_subitem
              input#gender_checkbox1.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox1") Male
            .form-item_subitem
              input#gender_checkbox2.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox2") Female
            .form-item_subitem
              input#gender_checkbox3.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox3") Transgender
            .form-item_subitem
              input#gender_checkbox4.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox4") LGBT
          
          .form-item.form-item__language
            label Language
            
            .form-item_subitem
              input#language_checkbox1.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox1") English
            .form-item_subitem
              input#language_checkbox2.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox2") English
            .form-item_subitem
              input#language_checkbox3.checkbox(type="checkbox" name="gender_checkbox")
              label(for="gender_checkbox3") Ukrainian
            .form-item_subitem
